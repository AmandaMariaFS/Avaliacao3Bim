@{
    ViewData["Title"] = "Resposta cadastro";
}

@if (ViewBag.Cadastrado)
{
    <h1>A loja @ViewBag.Name foi cadastrada com sucesso</h1>
}
else
{
    <h1>A loja @ViewBag.Name não foi cadastrada pois já existe alguma loja com o mesmo Id ou nome</h1>
}

<a href="/Shopping/Index"> Voltar para lista de lojas </a>